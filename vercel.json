{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "functions": {
    "api/send-email.js": {
      "memory": 1024,
      "maxDuration": 10
    }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/index.js"
      }
    ]
  }
  