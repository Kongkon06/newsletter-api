{
  "version": 2,
  "builds": [
    {
      "src": "api/send-email.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/send-email.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/send-email",
      "dest": "/api/send-email.js"
    }
  ]
}